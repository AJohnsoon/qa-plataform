<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Discuss</title>
    <link rel="stylesheet" type="text/css" href="/css/discuss.css">

</head>

<body>
    <%- include ('partials/header.ejs') %>
        <nav>
            <h1>interaction and comments </h1>
        </nav>
        <div class="folder">
            <header><%= foldQuestion.title %></header>
            <p><%= foldQuestion.description %></p>
            <hr>
            <h3>Comments:</h3>
            <% foldAnswer.forEach(answer =>{ %>
            <div class="response">                                   
                <p> <%= answer.comment %></p>                                 
            </div>
            <% }) %>
            <%- include ('partials/discuss.ejs') %>
        </div>
        

</body>

</html>